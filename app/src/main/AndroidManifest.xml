        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Pengaturan" android:parentActivityName=".Profile" android:label="@string/pengaturan"></activity>
        <activity
            android:name=".EditProfile"
            android:label="@string/edit_profil"
            android:parentActivityName=".Profile" />
        <activity
            android:name=".FormTambahKaryawan"
            android:label="@string/tambah_karyawan"
